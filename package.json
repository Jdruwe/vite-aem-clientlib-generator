{
  "name": "@jeroendruwe/vite-aem-clientlib-generator",
  "description": "Creates a AEM clientlib based on the manifest.json generated by Vite",
  "version": "0.0.0",
  "author": "Jeroen DruwÃ©",
  "bin": {
    "vite-aem-lib": "./bin/run"
  },
  "bugs": "https://github.com/Jdruwe/vite-aem-clientlib-generator/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "fs-extra": "^10.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^5",
    "@types/node": "^16.11.6",
    "@types/xml2js": "^0.4.9",
    "chai": "^4",
    "dir-compare": "^3.3.0",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Jdruwe/vite-aem-clientlib-generator",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vite-aem-lib",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Jdruwe/vite-aem-clientlib-generator",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
